<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#031424"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#031424"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="47dp">

            <ImageView
                android:layout_width="9dp"
                android:layout_height="16dp"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:layout_marginLeft="18dp"
                android:src="@mipmap/back_white" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="故障信息查询"
                android:textColor="#FFFFFF"
                android:textSize="16dp" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="649dp"
            android:background="@mipmap/image_log_message_check"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dp_21"
                    android:layout_height="@dimen/dp_22"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_48" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_49"
                    android:text="时间"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/sp_14" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/dp_109"
                    android:layout_height="@dimen/dp_31"
                    android:layout_marginLeft="@dimen/dp_46"
                    android:layout_marginTop="@dimen/dp_14"
                    android:background="@mipmap/start_time_back"
                    android:gravity="center"
                    android:text="开始时间"
                    android:textColor="#ffffff" />

                <View
                    android:layout_width="@dimen/dp_24"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/dp_9"
                    android:layout_marginTop="@dimen/dp_31"
                    android:background="#D0D0D0" />

                <TextView
                    android:layout_width="@dimen/dp_109"
                    android:layout_height="@dimen/dp_31"
                    android:layout_marginLeft="@dimen/dp_9"
                    android:layout_marginTop="@dimen/dp_14"
                    android:layout_marginRight="@dimen/dp_34"
                    android:background="@mipmap/start_time_back"
                    android:gravity="center"
                    android:text="结束时间"
                    android:textColor="#ffffff" />


            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_26">

                <ImageView
                    android:layout_width="@dimen/dp_21"
                    android:layout_height="@dimen/dp_22"
                    android:layout_marginLeft="@dimen/dp_20" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="日志模块"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/sp_14" />

                <ImageView
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_7"
                    android:layout_marginLeft="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_7"
                    android:src="@mipmap/icon_top" />
            </LinearLayout>

            <ExpandableListView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_122"
                />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_26">

                <ImageView
                    android:layout_width="@dimen/dp_21"
                    android:layout_height="@dimen/dp_22"
                    android:layout_marginLeft="@dimen/dp_20" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="日志类型"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/sp_14" />

                <ImageView
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_7"
                    android:layout_marginLeft="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_7"
                    android:src="@mipmap/icon_bottom" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_26">

                <ImageView
                    android:layout_width="@dimen/dp_21"
                    android:layout_height="@dimen/dp_22"
                    android:layout_marginLeft="@dimen/dp_20" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="任务编号"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/sp_14" />


            </LinearLayout>
            <TextView
                android:layout_width="@dimen/dp_243"
                android:layout_height="@dimen/dp_31"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/dp_14"
                android:textColor="#ffffff"
                android:paddingTop="@dimen/dp_8"
                android:textSize="@dimen/sp_12"
                android:text="请输入任务编号"
                android:paddingLeft="@dimen/dp_16"
                android:background="@mipmap/start_time_back"

                />

        </LinearLayout>
        <Button
            android:text="提交"
            android:textColor="#ffffff"
            android:textSize="13sp"
            android:layout_marginTop="5dp"
            android:layout_width="@dimen/dp_324"
            android:layout_height="@dimen/dp_38"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/dp_11"
            android:background="@mipmap/image_btn_fault_message_back"
            />
    </LinearLayout>


</androidx.core.widget.NestedScrollView>